    				p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "BREWING " + ChatColor.GRAY + "This cauldron is brewing an awkward potion. Shift-Right-Click to retrieve this potion.");
    			
    			} else if (p.getClickedBlock().hasMetaData("SpeedNormal")) {
    			
    				p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "BREWING " + ChatColor.GRAY + "This cauldron is brewing a normal speed potion. Shift-Right-Click to retrieve this potion.");
    			
    			} else if (p.getClickedBlock().hasMetaData("SpeedEnhanced")) {
    			
    				p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "BREWING " + ChatColor.GRAY + "This cauldron is brewing an enhanced speed potion. Shift-Right-Click to retrieve this potion.");
    			
    			} else if (p.getClickedBlock().hasMetaData("SpeedExtended")) {
    			
    				p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "BREWING " + ChatColor.GRAY + "This cauldron is brewing an extended speed potion. Shift-Right-Click to retrieve this potion.");
    			
    			} else if (p.getClickedBlock().hasMetaData("StrengthNormal")) {
    			
    				p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "BREWING " + ChatColor.GRAY + "This cauldron is brewing a normal strength potion. Shift-Right-Click to retrieve this potion.");
    			
    			} else if (p.getClickedBlock().hasMetaData("StrengthEnhanced")) {
    			
    				p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "BREWING " + ChatColor.GRAY + "This cauldron is brewing an enhanced strength potion. Shift-Right-Click to retrieve this potion.");
    			
    			} else if (p.getClickedBlock().hasMetaData("StrengthExtended")) {
    			
    				p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "BREWING " + ChatColor.GRAY + "This cauldron is brewing an extended strength potion. Shift-Right-Click to retrieve this potion.");
    			
    			} else if (p.getClickedBlock().hasMetaData("HealingNormal")) {
    			
    				p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "BREWING " + ChatColor.GRAY + "This cauldron is brewing a normal healing potion. Shift-Right-Click to retrieve this potion.");
    			
    			} else if (p.getClickedBlock().hasMetaData("HealingEnhanced")) {
    			
    				p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "BREWING " + ChatColor.GRAY + "This cauldron is brewing an enhanced healing potion. Shift-Right-Click to retrieve this potion.");
    			
    			} else if (p.getClickedBlock().hasMetaData("HealingEnhancedSplash")) {
    			
    				p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "BREWING " + ChatColor.GRAY + "This cauldron is brewing an enhanced splash healing potion. Shift-Right-Click to retrieve this potion.");
    			
    			} else if (p.getClickedBlock().hasMetaData("FResNormal")) {
    			
    				p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "BREWING " + ChatColor.GRAY + "This cauldron is brewing a normal fire res potion. Shift-Right-Click to retrieve this potion.");
    			
    			} else if (p.getClickedBlock().hasMetaData("FResExtended")) {
    			
    				p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "BREWING " + ChatColor.GRAY + "This cauldron is brewing an extended fire res potion. Shift-Right-Click to retrieve this potion.");
    			
    			} else if (p.getClickedBlock().hasMetaData("RegenNormal")) {
    			
    				p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "BREWING " + ChatColor.GRAY + "This cauldron is brewing a normal regen potion. Shift-Right-Click to retrieve this potion.");
    			
    			} else if (p.getClickedBlock().hasMetaData("RegenExtended")) {
    			
    				p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "BREWING " + ChatColor.GRAY + "This cauldron is brewing an extended regen potion. Shift-Right-Click to retrieve this potion.");
    			
    			} else if (p.getClickedBlock().hasMetaData("RegenEnhanced")) {
    			
    				p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "BREWING " + ChatColor.GRAY + "This cauldron is brewing an enhanced regen potion. Shift-Right-Click to retrieve this potion.");
    			
    			}
    		
    		}
    	
    	}
    	
    }

    @SuppressWarnings("deprecation")
	@EventHandler
    public void onInventoryClick(InventoryClickEvent e) {

        Player p = (Player) e.getWhoClicked();

        ItemStack str = new ItemStack(Material.POTION, 1, (short) 8233);
        PotionMeta strMeta = (PotionMeta) str.getItemMeta();
        strMeta.addCustomEffect(new PotionEffect(PotionEffectType.INCREASE_DAMAGE, 4800, 1), true);
        str.setItemMeta(strMeta);
        
        ItemStack str1 = new ItemStack(Material.POTION, 2, (short) 8233);
        PotionMeta str1Meta = (PotionMeta) str1.getItemMeta();
        str1Meta.addCustomEffect(new PotionEffect(PotionEffectType.INCREASE_DAMAGE, 4800, 1), true);
        str1.setItemMeta(str1Meta);
        
        ItemStack spe = new ItemStack(Material.POTION, 1, (short) 8226);
        PotionMeta speMeta = (PotionMeta) spe.getItemMeta();
        speMeta.addCustomEffect(new PotionEffect(PotionEffectType.SPEED, 4800, 1), true);
        spe.setItemMeta(speMeta);
        
        ItemStack spe1 = new ItemStack(Material.POTION, 1, (short) 8226);
        PotionMeta spe1Meta = (PotionMeta) spe1.getItemMeta();
        spe1Meta.addCustomEffect(new PotionEffect(PotionEffectType.SPEED, 4800, 1), true);
        spe1.setItemMeta(spe1Meta);
        
        if (e.getCurrentItem() == str && e.getCursor() == str) {
        	
        	e.setCancelled(true);
        	
        	e.setCurrentItem(str1);
        	e.setCursor(new ItemStack(Material.AIR));
        	
        }
        
        if (e.getCurrentItem() == spe && e.getCursor() == spe) {
        	
        	e.setCancelled(true);
        	
        	e.setCurrentItem(spe1);
        	e.setCursor(new ItemStack(Material.AIR));
        	
        }

        if (e.getInventory().getName().equalsIgnoreCase(ChatColor.BLUE + "" + ChatColor.BOLD + "Redeem Your Kit")) {

            e.setCancelled(true);

            if (e.getCurrentItem().getItemMeta().getDisplayName().equalsIgnoreCase(ChatColor.DARK_GRAY + "" + ChatColor.BOLD + "Kit Starter")) {

                if (starterTime.containsKey(p)) {

                    p.closeInventory();
                    p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "KITS " + ChatColor.GRAY + "This kit is still on cooldown for " + ChatColor.UNDERLINE + starterTime.get(p) + ChatColor.GRAY + " hours.");
                    return;

                }

                p.closeInventory();

                p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "KITS " + ChatColor.GRAY + "You have redeemed your " + ChatColor.UNDERLINE + "Starter Kit" + ChatColor.GRAY + ".");

                ItemStack sword = new ItemStack(Material.DIAMOND_SWORD, 1);
                sword.addEnchantment(Enchantment.DAMAGE_ALL, 1);
                ItemMeta swordMeta = sword.getItemMeta();
                swordMeta.setDisplayName(ChatColor.GRAY + "[Starter] " + ChatColor.WHITE + "Sword");
                sword.setItemMeta(swordMeta);

                ItemStack helmet = new ItemStack(Material.IRON_HELMET, 1);
                helmet.addEnchantment(Enchantment.PROTECTION_ENVIRONMENTAL, 1);
                ItemMeta helmetMeta = helmet.getItemMeta();
                helmetMeta.setDisplayName(ChatColor.GRAY + "[Starter] " + ChatColor.WHITE + "Helmet");
                helmet.setItemMeta(helmetMeta);

                ItemStack chestplate = new ItemStack(Material.IRON_CHESTPLATE, 1);
                chestplate.addEnchantment(Enchantment.PROTECTION_ENVIRONMENTAL, 1);
                ItemMeta chestplateMeta = chestplate.getItemMeta();
                chestplateMeta.setDisplayName(ChatColor.GRAY + "[Starter] " + ChatColor.WHITE + "Chestplate");
                chestplate.setItemMeta(chestplateMeta);

                ItemStack leggings = new ItemStack(Material.IRON_LEGGINGS, 1);
                leggings.addEnchantment(Enchantment.PROTECTION_ENVIRONMENTAL, 1);
                ItemMeta leggingsMeta = leggings.getItemMeta();
                leggingsMeta.setDisplayName(ChatColor.GRAY + "[Starter] " + ChatColor.WHITE + "Leggings");
                leggings.setItemMeta(leggingsMeta);

                ItemStack boots = new ItemStack(Material.IRON_BOOTS, 1);
                boots.addEnchantment(Enchantment.PROTECTION_ENVIRONMENTAL, 1);
                ItemMeta bootsMeta = boots.getItemMeta();
                bootsMeta.setDisplayName(ChatColor.GRAY + "[Starter] " + ChatColor.WHITE + "Boots");
                boots.setItemMeta(bootsMeta);

                ItemStack pickaxe = new ItemStack(Material.DIAMOND_PICKAXE, 1);
                pickaxe.addEnchantment(Enchantment.DIG_SPEED, 1);
                ItemMeta pickaxeMeta = pickaxe.getItemMeta();
                pickaxeMeta.setDisplayName(ChatColor.GRAY + "[Starter] " + ChatColor.WHITE + "Pickaxe");
                pickaxe.setItemMeta(pickaxeMeta);

                ItemStack bow = new ItemStack(Material.BOW, 1);
                bow.addEnchantment(Enchantment.ARROW_DAMAGE, 1);
                ItemMeta bowMeta = bow.getItemMeta();
                bowMeta.setDisplayName(ChatColor.GRAY + "[Starter] " + ChatColor.WHITE + "Bow");
                bow.setItemMeta(bowMeta);

                ItemStack arrows = new ItemStack(Material.ARROW, 32);

                ItemStack crapples = new ItemStack(Material.GOLDEN_APPLE, 4);

                ItemStack tnt = new ItemStack(Material.TNT, 32);

                ItemStack obby = new ItemStack(Material.OBSIDIAN, 32);

                ItemStack stone = new ItemStack(Material.STONE, 64);

                ItemStack diamonds = new ItemStack(Material.DIAMOND, 8);

                ItemStack iron = new ItemStack(Material.IRON_INGOT, 12);
                
                ItemStack exp = new ItemStack(Material.EXP_BOTTLE, 16);
                
                p.getInventory().addItem(sword);
                p.getInventory().addItem(helmet);
                p.getInventory().addItem(chestplate);
                p.getInventory().addItem(leggings);
                p.getInventory().addItem(boots);
                p.getInventory().addItem(pickaxe);
                p.getInventory().addItem(bow);
                p.getInventory().addItem(arrows);
                p.getInventory().addItem(crapples);
                p.getInventory().addItem(tnt);
                p.getInventory().addItem(obby);
                p.getInventory().addItem(stone);
                p.getInventory().addItem(exp);
                p.getInventory().addItem(diamonds);
                p.getInventory().addItem(iron);
                p.getInventory().addItem(str);
                p.getInventory().addItem(spe);

                starterTime.put(p, 24);
                starterTask.put(p, new BukkitRunnable() {

                    public void run() {

                        starterTime.put(p, starterTime.get(p) - 1);

                        if (starterTime.get(p) == 0) {

                            starterTime.remove(p);
                            starterTask.remove(p);

                            cancel();

                        }

                    }

                });

                starterTask.get(p).runTaskTimer(this, 72000, 72000);

            } else if (e.getCurrentItem().getItemMeta().getDisplayName().equalsIgnoreCase(ChatColor.GREEN + "" + ChatColor.BOLD + "Kit Farmer")) {

                if (perms.getInt("Kit." + p.getUniqueId().toString()) == 1) {

                    if (farmerTime.containsKey(p)) {

                        p.closeInventory();
                        p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "KITS " + ChatColor.GRAY + "This kit is still on cooldown for " + ChatColor.UNDERLINE + farmerTime.get(p) + ChatColor.GRAY + " hours.");
                        return;

                    }

                    p.closeInventory();

                    p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "KITS " + ChatColor.GRAY + "You have redeemed your " + ChatColor.UNDERLINE + "Farmer Kit" + ChatColor.GRAY + ".");

                    ItemStack sword = new ItemStack(Material.DIAMOND_SWORD, 1);
                    sword.addEnchantment(Enchantment.DAMAGE_ALL, 2);
                    sword.addEnchantment(Enchantment.FIRE_ASPECT, 1);
                    ItemMeta swordMeta = sword.getItemMeta();
                    swordMeta.setDisplayName(ChatColor.GRAY + "[" + ChatColor.GREEN + "Farmer" + ChatColor.GRAY + "] " + ChatColor.WHITE + "Sword");
                    sword.setItemMeta(swordMeta);

                    ItemStack helmet = new ItemStack(Material.IRON_HELMET, 1);
                    helmet.addEnchantment(Enchantment.PROTECTION_ENVIRONMENTAL, 4);
                    helmet.addEnchantment(Enchantment.DURABILITY, 2);
                    ItemMeta helmetMeta = helmet.getItemMeta();
                    helmetMeta.setDisplayName(ChatColor.GRAY + "[" + ChatColor.GREEN + "Farmer" + ChatColor.GRAY + "] " + ChatColor.WHITE + "Helmet");
                    helmet.setItemMeta(helmetMeta);

                    ItemStack chestplate = new ItemStack(Material.IRON_CHESTPLATE, 1);
                    chestplate.addEnchantment(Enchantment.PROTECTION_ENVIRONMENTAL, 4);
