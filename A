                    swordMeta.setDisplayName(ChatColor.GRAY + "[" + ChatColor.GOLD + "Industrialist" + ChatColor.GRAY + "] " + ChatColor.WHITE + "Sword");
                    sword.setItemMeta(swordMeta);

