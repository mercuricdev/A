                    p.closeInventory();

                    p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "KITS " + ChatColor.GRAY + "You have redeemed your " + ChatColor.UNDERLINE + "Industrialist Kit" + ChatColor.GRAY + ".");

