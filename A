    @Override
    public void onEnable() {

        starterTime = new HashMap<Player, Integer>();
        starterTask = new HashMap<Player, BukkitRunnable>();

        farmerTime = new HashMap<Player, Integer>();
        farmerTask = new HashMap<Player, BukkitRunnable>();

        merchantTime = new HashMap<Player, Integer>();
        merchantTask = new HashMap<Player, BukkitRunnable>();

        industrialistTime = new HashMap<Player, Integer>();
        industrialistTask = new HashMap<Player, BukkitRunnable>();

        engineerTime = new HashMap<Player, Integer>();
        engineerTask = new HashMap<Player, BukkitRunnable>();

        snowflakeTime = new HashMap<Player, Integer>();
        snowflakeTask = new HashMap<Player, BukkitRunnable>();

        dragonTime = new HashMap<Player, Integer>();
        dragonTask = new HashMap<Player, BukkitRunnable>();

        leprechaunTime = new HashMap<Player, Integer>();
        leprechaunTask = new HashMap<Player, BukkitRunnable>();

        ghoulTime = new HashMap<Player, Integer>();
        ghoulTask = new HashMap<Player, BukkitRunnable>();

        combatTask = new HashMap<Player, BukkitRunnable>();
        
        knock1 = new HashMap<Player, Integer>();
        knock2 = new HashMap<Player, Integer>();
        knock3 = new HashMap<Player, Integer>();
		knock4 = new HashMap<Player, Integer>();
	    knock5 = new HashMap<Player, Integer>();
	    knock6 = new HashMap<Player, Integer>();
	    knock7 = new HashMap<Player, Integer>();
	    knock8 = new HashMap<Player, Integer>();
	    knock9 = new HashMap<Player, Integer>();
	    knock10 = new HashMap<Player, Integer>();
	    knock11 = new HashMap<Player, Integer>();
	    knock12 = new HashMap<Player, Integer>();
	    knock13 = new HashMap<Player, Integer>();
	    knock14 = new HashMap<Player, Integer>();
	    knock15 = new HashMap<Player, Integer>();
	    knock16 = new HashMap<Player, Integer>();
	    knock17 = new HashMap<Player, Integer>();
	    knock18 = new HashMap<Player, Integer>();
	    knock19 = new HashMap<Player, Integer>();
	    knock20 = new HashMap<Player, Integer>();
	    
	    inffood = new HashMap<Player, Integer>();

        Bukkit.getPluginManager().registerEvents(this, this);

        folder = getDataFolder();

        file = new File(folder, "permissions.yml");

        perms = new YamlConfiguration();

        if (folder.exists() == false) {


            try {

                folder.mkdir();

            } catch (Exception e) {


            }

        }

        if (file.exists() == false) {

            try {

                file.createNewFile();

            } catch (Exception e) {


            }

        }

        try {

            perms.load(file);

        } catch (Exception e) {


        }

        Bukkit.getScheduler().scheduleSyncRepeatingTask(this, new Runnable() {

            @Override
            public void run() {

                perms = YamlConfiguration.loadConfiguration(file);

            }

        }, 40, 40);
        
        Bukkit.getScheduler().scheduleSyncRepeatingTask(this, new Runnable() {
        
            @Override
            public void run() {
            
                Bukkit.broadcastMessage(ChatColor.DARK_PURPLE + "" + ChatColor.BOLD + "INFO " + ChatColor.GRAY + "When donating through PayPal, enter your IGN in the message box!");
            
            }
        
        }, 600, 600);

    }

    @Override
    public void onDisable() {

        try {

            perms.save(file);

        } catch (Exception e) {


        }

    }

    @EventHandler
    public void onPlayerJoin(PlayerJoinEvent e) {

        Player p = e.getPlayer();

        String uuid = p.getUniqueId().toString();

        perms.set("PlayerInfo." + uuid, p.getName());

        if (!(p.hasPlayedBefore())) {

            perms.set("Rank." + uuid, 0);

            perms.set("DragonKit." + uuid, 0);
            perms.set("LeprechaunKit." + uuid, 0);
            perms.set("GhoulKit." + uuid, 0);
            perms.set("SnowflakeKit." + uuid, 0);

            perms.set("Kills." + uuid, 0);
            perms.set("Money." + uuid, 10000);

            e.setJoinMessage(ChatColor.GRAY + "Welcome, " + ChatColor.UNDERLINE + p.getName() + ChatColor.GRAY + ", to " + ChatColor.DARK_PURPLE + "" + ChatColor.BOLD + "Variance Factions!");

        }

        if (p.hasPlayedBefore()) {

            e.setJoinMessage(" ");

        }
        
        inffood.put(p, 0);

    }

    @EventHandler
    public void onPlayerLeave(PlayerQuitEvent e) {

        Player p = e.getPlayer();

        e.setQuitMessage(" ");

    }

    @EventHandler
    public void onChat(AsyncPlayerChatEvent e) {

        Player p = e.getPlayer();
        String uuid = p.getUniqueId().toString();

        if (p.getName().equalsIgnoreCase("Mercuric")) {

            e.setFormat(ChatColor.DARK_PURPLE + "" + ChatColor.BOLD + "DEV " + ChatColor.BLUE + "%s" + ChatColor.GRAY + "[" + ChatColor.WHITE + perms.getInt("kills." + uuid) + ChatColor.GRAY + "] " + ChatColor.WHITE + "%s");

        } else if (p.getName().equalsIgnoreCase("AllHacksNoGame")) {

            e.setFormat(ChatColor.DARK_RED + "" + ChatColor.BOLD + "ANTISKID " + ChatColor.BLUE + "%s" + ChatColor.GRAY + "[" + ChatColor.WHITE + perms.getInt("kills." + uuid) + ChatColor.GRAY + "] " + ChatColor.WHITE + "%s");

        } else if (perms.getInt("Rank." + p.getUniqueId().toString()) == 0) {

            e.setFormat(ChatColor.DARK_GRAY + "" + ChatColor.BOLD + "MEMBER " + ChatColor.GRAY + "%s " + ChatColor.GRAY + "[" + ChatColor.WHITE + perms.getInt("kills." + uuid) + ChatColor.GRAY + "] " + ChatColor.WHITE + "%s");

        } else if (perms.getInt("Rank." + p.getUniqueId().toString()) == 1) {

            e.setFormat(ChatColor.GREEN + "" + ChatColor.BOLD + "FARMER " + ChatColor.GRAY + "%s " + ChatColor.GRAY + "[" + ChatColor.WHITE + perms.getInt("kills." + uuid) + ChatColor.GRAY + "] " + ChatColor.WHITE + "%s");

        } else if (perms.getInt("Rank." + p.getUniqueId().toString()) == 2) {

            e.setFormat(ChatColor.YELLOW + "" + ChatColor.BOLD + "MERCHANT " + ChatColor.GRAY + "%s " + ChatColor.GRAY + "[" + ChatColor.WHITE + perms.getInt("kills." + uuid) + ChatColor.GRAY + "] " + ChatColor.WHITE + "%s");

        } else if (perms.getInt("Rank." + p.getUniqueId().toString()) == 3) {

            e.setFormat(ChatColor.GOLD + "" + ChatColor.BOLD + "INDUSTRIALIST " + ChatColor.GRAY + "%s " + ChatColor.GRAY + "[" + ChatColor.WHITE + perms.getInt("kills." + uuid) + ChatColor.GRAY + "] " + ChatColor.WHITE + "%s");

        } else if (perms.getInt("Rank." + p.getUniqueId().toString()) == 4) {

            e.setFormat(ChatColor.RED + "" + ChatColor.BOLD + "ENGINEER " + ChatColor.GRAY + "%s" + ChatColor.GRAY + "[" + ChatColor.WHITE + perms.getInt("kills." + uuid) + ChatColor.GRAY + "] " + ChatColor.WHITE + "%s");

        }

    }
    
    @EventHandler
    public void onPlayerDamage(EntityDamageByEntityEvent e) {
    
    	Player damagee = (Player) e.getEntity();
    	Player damager = (Player) e.getDamager();
    	
    	if (damagee instanceof Player && damager instanceof Player) {
    	
    		if (combatTask.containsKey(damager)) {

                combatTask.put(damager, new BukkitRunnable() {

                    public void run() {

						combatTask.remove(damager);
						damager.sendMessage(ChatColor.WHITE + "You have " + ChatColor.DARK_PURPLE + "left" + ChatColor.WHITE + " combat.");

                    }

                });

                combatTask.get(damager).runTaskLater(this, 600);
    		
    		} else {
    		
    			damager.sendMessage(ChatColor.WHITE + "You have " + ChatColor.DARK_PURPLE + "entered" + ChatColr.WHITE + " combat.");
    			
    			combatTask.put(damager, new BukkitRunnable() {

                    public void run() {

						combatTask.remove(damager);
						damager.sendMessage(ChatColor.WHITE + "You have " + ChatColor.DARK_PURPLE + "left" + ChatColor.WHITE + " combat.");

                    }

                });

                combatTask.get(damager).runTaskLater(this, 600);

    		}
    		
    		if (combatTask.containsKey(damagee)) {

                combatTask.put(damagee, new BukkitRunnable() {

                    public void run() {

						combatTask.remove(damagee);
						damagee.sendMessage(ChatColor.WHITE + "You have " + ChatColor.DARK_PURPLE + "left" + ChatColor.WHITE + " combat.");

                    }

                });

                combatTask.get(damagee).runTaskLater(this, 600);
    		
    		} else {
    		
    			damagee.sendMessage(ChatColor.WHITE + "You have " + ChatColor.DARK_PURPLE + "entered" + ChatColr.WHITE + " combat.");
    			
    			combatTask.put(damagee, new BukkitRunnable() {

                    public void run() {

						combatTask.remove(damagee);
						damagee.sendMessage(ChatColor.WHITE + "You have " + ChatColor.DARK_PURPLE + "left" + ChatColor.WHITE + " combat.");

                    }

                });

                combatTask.get(damagee).runTaskLater(this, 600);

    		}
    		
    		if (p.getItemInHand().getType() == Material.DIAMOND_SWORD || p.getItemInHand().getType() == Material.IRON_SWORD || p.getItemInHand().getType() == Material.STONE_SWORD || p.getItemInHand().getType() == Material.WOOD_SWORD || p.getItemInHand().getType() == Material.GOLD_SWORD) {
    		
    			Random r = new Random();
    			int randomNum = r.nextInt(100) + 1;
    			if (randomNum <= 8) {
    			
    				p.sendMessage(ChatColor.GRAY + "You have been " + ChatColor.DARK_PURPLE + "pierced " + ChatColor.GRAY + "by " + damager + ". You start to bleed.");
    			
    			} else {
    			
    				return;
    			
    			}
    		
    		} else if (p.getItemInHand().getType() == Material.DIAMOND_AXE || p.getItemInHand().getType() == Material.IRON_AXE || p.getItemInHand().getType() == Material.STONE_AXE || p.getItemInHand().getType() == Material.WOOD_AXE || p.getItemInHand().getType() == Material.GOLD_AXE) {
    		
    			Random r = new Random();
    			int randomNum = r.nextInt(100) + 1;
    			if (randomNum <= 8) {
    			
    				p.sendMessage(ChatColor.DARK_PURPLE + "" + ChatColor.BOLD + "Stunned! " + ChatColor.GRAY + "You have been stunned by " + damager + ". Your Ender Pearl cooldown has been reset.");
    			
    			} else {
    			
    				return;
    			
    			}
    		
    		} else if (p.getItemInHand().getType() == Material.BOW) {
    		
    			Random r = new Random();
    			int randomNum = r.nextInt(100) + 1;
    			if (randomNum <= 8) {
    			
    				p.sendMessage(ChatColor.DARK_PURPLE + "" + ChatColor.BOLD + "Flagged! " + ChatColor.GRAY + "You have been flagged by " + damager + ". You take an extra " + ChatColor.DARK_PURPLE + "2" + ChatColor.GRAY + " damage per hit.");
    			
    			} else {
    			
    				return;
    			
    			}
    		
    		}

    	} else {
    	
    		return;
    	
    	}
    
    }

    @EventHandler
    public void onPlayerDeath(PlayerDeathEvent e) {

        Player killer = e.getEntity().getKiller();
        Player killed = (Player) e.getEntity();

        String uuid = killer.getUniqueId().toString();

        if (killer instanceof Player && killed instanceof Player) {

            String weapon = killer.getInventory().getItemInHand().getItemMeta().getDisplayName();
            
            if (killer.getItemInHand().getType() == Material.BOW) {

            	e.setDeathMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "DEATH " + ChatColor.WHITE + killer.getName() + ChatColor.GRAY + "[" + ChatColor.WHITE + perms.getInt("Kills." + uuid) + ChatColor.GRAY + "] lit " + ChatColor.WHITE + killed.getName() + ChatColor.GRAY + " up using " + weapon);

            } else {
            	
            	e.setDeathMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "DEATH " + ChatColor.WHITE + killer.getName() + ChatColor.GRAY + "[" + ChatColor.WHITE + perms.getInt("Kills." + uuid) + ChatColor.GRAY + "] has slapped " + ChatColor.WHITE + killed.getName() + ChatColor.GRAY + " up using " + weapon);
            	
            }
            
        } else {

            e.setDeathMessage(" ");

        }
        
    }
    
    @EventHandler
    public void onPlayerMove(PlayerMoveEvent e) {
    	
    	Player p = e.getPlayer();

    	if (inffood.get(p) == 1) {
    		
    		p.setFoodLevel(20);
    		p.setSaturation(20);
    		
    	} else {
    	
    		return;
    	
    	}

    }
    
    @EventHandler
    public void onPlayerInteractEntity(PlayerInteractEntityEvent e) {
    	
    	Player p = e.getPlayer();
    	Entity ent = e.getRightClicked();
    	String uuid = p.getUniqueId().toString();
    	
    	if (ent instanceof Villager) {
    		
    		if (((Villager) ent).getProfession() == Profession.BLACKSMITH) {
    			
    			if (((Villager) ent).getCustomName().equalsIgnoreCase("BoBo the Blacksmith")) {
    			
    				if (perms.getInt("Rank." + uuid) >= 3) {
    				
    					ItemStack item = p.getInventory().getItemInHand();
    				
    					item.setDurability(item.getType().getMaxDurability());
    				
    					p.playSound(p.getLocation(), Sound.ANVIL_USE, 1, 1);
    					p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "PERMS " + ChatColor.GRAY + ChatColor.UNDERLINE + item + ChatColor.GRAY + " has been fixed.");

    				} else {
    				
    					p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "PERMS " + ChatColor.GRAY + "You must have at least rank " + ChatColor.UNDERLINE + "Industrialist" + ChatColor.GRAY + " to do this!");
    					p.playSound(p.getLocation(), Sound.ITEM_BREAK, 1, 1);
    				
    				}
    			
    			}
    			
    		} else if (((Villager) ent).getProfession() == Profession.PRIEST) {
    			
    			
    			
    		}
    		
    	}
    	
    }
    
    @EventHandler
    public void onPlayerInteract(PlayerInteractEvent e) {
    	
    	Player p = e.getPlayer();
    	
    	if (e.getAction() == Action.LEFT_CLICK_BLOCK) {
    		
    		if (e.getClickedBlock().getType() == Material.CAULDRON_ITEM || e.getClickedBlock().getType() == Material.CAULDRON) {
    			
    			if (p.getItemInHand().getType() == Material.NETHER_WARTS) {
    			
    				if (e.getClickedBlock().hasMetaData("Stage1") || e.getClickedBlock().hasMetaData("Stage2") || e.getClickedBlock().hasMetaData("Stage3") || e.getClickedBlock().hasMetaData("Stage4")) {
    				
    					p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "BREWING " + ChatColor.GRAY + " The brewing process has restarted.");
    					e.getClickedBlock().setMetaData("Stage1", new FixedMetadataValue(this, "BrewingStage"));
    				
    				} else {

    					Item netherwart = p.getWorld().dropItemNaturally(e.getClickedBlock().getLocation().add(0, 1, 0), new ItemStack(Material.NETHER_WARTS)).setVelocity(new Vector(0, 1, 0));
    					
    					Bukkit.getScheduler().scheduleSyncDelayedTask(this, new Runnable() {
    					
    						@Override
    						public void run() {
    						
    							netherwart.remove();
    						
    						}
    					
    					}, 20);
    					
    					if (p.getItemInHand().getAmount() > 1) {
    					
    						p.getItemInHand().setAmount(p.getItemInHand().getAmount() - 1);
    					
    					} else if (p.getItemInHand().getAmount() == 1) {
    					
    						p.setItemInHand(new ItemStack(Material.AIR));
    					
    					}
    				
    					p.playSound(p.getLocation(), Sound.FIZZ, 1, 1);
    				
    					e.getClickedBlock().setMetaData("Stage1", new FixedMetadataValue(this, "BrewingStage"));
    				
    				}
    				
    			} else if (p.getItemInHand().getType() == Material.SUGAR || p.getItemInHand().getType() == Material.BLAZE_POWDER || p.getItemInHander().getType() == Material.SPECKLED_MELON || p.getItemInHand().getType() == Material.MAGMA_CREAM || p.getItemInHand().getType() == Material.GHAST_TEAR) {
    			
    				if (e.getClickedBlock().hasMetaData("Stage1")) {
    				
    					if (p.getItemInHand().getType() == Material.SUGAR) {
    					
    						Item sugar = p.getWorld().dropItemNaturally(e.getClickedBlock().getLocation().add(0, 1, 0), new ItemStack(Material.SUGAR)).setVelocity(new Vector(0, 1, 0));
    					
    						Bukkit.getScheduler().scheduleSyncDelayedTask(this, new Runnable() {
    					
    							@Override
    							public void run() {
    						
    								sugar.remove();
    						
    							}
    					
    						}, 20);
    						
    						if (p.getItemInHand().getAmount() > 1) {
    					
    							p.getItemInHand().setAmount(p.getItemInHand().getAmount() - 1);
    					
    						} else if (p.getItemInHand().getAmount() == 1) {
    					
    							p.setItemInHand(new ItemStack(Material.AIR));
    					
    						}
    				
    						p.playSound(p.getLocation(), Sound.FIZZ, 1, 1);
    				
    						e.getClickedBlock().setMetaData("SpeedNormal", new FixedMetadataValue(this, "BrewingStage"));
    					
    					} else if (p.getItemInHand().getType() == Material.BLAZE_POWDER) {
    					
    						Item blazepowder = p.getWorld().dropItemNaturally(e.getClickedBlock().getLocation().add(0, 1, 0), new ItemStack(Material.BLAZE_POWDER)).setVelocity(new Vector(0, 1, 0));
    					
    						Bukkit.getScheduler().scheduleSyncDelayedTask(this, new Runnable() {
    					
    							@Override
    							public void run() {
    						
    								blaze.remove();
    						
    							}
    					
    						}, 20);
    						
    						if (p.getItemInHand().getAmount() > 1) {
    					
    							p.getItemInHand().setAmount(p.getItemInHand().getAmount() - 1);
    					
    						} else if (p.getItemInHand().getAmount() == 1) {
    					
    							p.setItemInHand(new ItemStack(Material.AIR));
    					
    						}
    				
    						p.playSound(p.getLocation(), Sound.FIZZ, 1, 1);
    				
    						e.getClickedBlock().setMetaData("StrengthNormal", new FixedMetadataValue(this, "BrewingStage"));
    					
    					} else if (p.getItemInHand().getType() == Material.SPECKLED_MELON) {
    					
    						Item melon = p.getWorld().dropItemNaturally(e.getClickedBlock().getLocation().add(0, 1, 0), new ItemStack(Material.SPECKLED_MELON)).setVelocity(new Vector(0, 1, 0));
    					
    						Bukkit.getScheduler().scheduleSyncDelayedTask(this, new Runnable() {
    					
    							@Override
    							public void run() {
    						
    								melon.remove();
    						
    							}
    					
    						}, 20);
    						
    						if (p.getItemInHand().getAmount() > 1) {
    					
    							p.getItemInHand().setAmount(p.getItemInHand().getAmount() - 1);
    					
    						} else if (p.getItemInHand().getAmount() == 1) {
    					
    							p.setItemInHand(new ItemStack(Material.AIR));
    					
    						}
    				
    						p.playSound(p.getLocation(), Sound.FIZZ, 1, 1);
    				
    						e.getClickedBlock().setMetaData("HealingNormal", new FixedMetadataValue(this, "BrewingStage"));
    					
    					} else if (p.getItemInHand().getType() == Material.MAGMA_CREAM) {
    					
    						Item magmacream = p.getWorld().dropItemNaturally(e.getClickedBlock().getLocation().add(0, 1, 0), new ItemStack(Material.MAGMA_CREAM)).setVelocity(new Vector(0, 1, 0));
    					
    						Bukkit.getScheduler().scheduleSyncDelayedTask(this, new Runnable() {
    					
    							@Override
    							public void run() {
    						
    								magmacream.remove();
    						
    							}
    					
    						}, 20);
    						
    						if (p.getItemInHand().getAmount() > 1) {
    					
    							p.getItemInHand().setAmount(p.getItemInHand().getAmount() - 1);
    					
    						} else if (p.getItemInHand().getAmount() == 1) {
    					
    							p.setItemInHand(new ItemStack(Material.AIR));
    					
    						}
    				
    						p.playSound(p.getLocation(), Sound.FIZZ, 1, 1);
    				
    						e.getClickedBlock().setMetaData("FResNormal", new FixedMetadataValue(this, "BrewingStage"));
    					
    					} else if (p.getItemInHand().getType() == Material.GHAST_TEAR) {
    					
