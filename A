                    boots.setItemMeta(bootsMeta);

                    ItemStack gapples = new ItemStack(Material.GOLDEN_APPLE, 1, (short) 1);

                    ItemStack crapples = new ItemStack(Material.GOLDEN_APPLE, 24);

                    ItemStack tnt = new ItemStack(Material.TNT, 64);

                    ItemStack obby = new ItemStack(Material.OBSIDIAN, 64);

                    ItemStack diamonds = new ItemStack(Material.DIAMOND, 32);

                    ItemStack iron = new ItemStack(Material.IRON_INGOT, 64);
                    
