                    p.getInventory().addItem(str);
                    p.getInventory().addItem(spe);
                    p.getInventory().addItem(str);
                    p.getInventory().addItem(spe);

                    leprechaunTime.put(p, 48);
                    leprechaunTask.put(p, new BukkitRunnable() {

                        public void run() {

                            leprechaunTime.put(p, leprechaunTime.get(p) - 1);

                            if (leprechaunTime.get(p) == 0) {

                                leprechaunTime.remove(p);
                                leprechaunTask.remove(p);

                                cancel();

                            }

                        }

                    });

                    leprechaunTask.get(p).runTaskTimer(this, 72000, 72000);

                } else {

                    p.closeInventory();

                    p.sendMessage(ChatColor.LIGHT_PURPLE + "" + ChatColor.BOLD + "KITS " + ChatColor.GRAY + "You do not have permissions for the " + ChatColor.UNDERLINE + "Leprechaun Kit" + ChatColor.GRAY + ".");

                }

            } else if (e.getCurrentItem().getItemMeta().getDisplayName().equalsIgnoreCase(ChatColor.DARK_PURPLE + "" + ChatColor.BOLD + "Kit Ghoul")) {

                if (perms.getInt("GhoulKit." + p.getUniqueId().toString()) == 1) {

