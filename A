                }

            } else if (e.getCurrentItem().getItemMeta().getDisplayName().equalsIgnoreCase(ChatColor.GOLD + "" + ChatColor.BOLD + "Kit Industrialist")) {

                if (perms.getInt("Kit." + p.getUniqueId().toString()) == 3) {

                    if (industrialistTime.containsKey(p)) {

                        p.closeInventory();
